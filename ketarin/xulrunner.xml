    <HttpReferer />
    <SetupInstructions />
    <Variables>
      <item>
        <key>
          <string>nopush</string>
        </key>
        <value>
          <UrlVariable>
            <RegexRightToLeft>false</RegexRightToLeft>
            <VariableType>Textual</VariableType>
            <Regex />
            <TextualContent>--nopush</TextualContent>
            <Name>nopush</Name>
          </UrlVariable>
        </value>
      </item>
      <item>
        <key>
          <string>version</string>
        </key>
        <value>
          <UrlVariable>
            <RegexRightToLeft>false</RegexRightToLeft>
            <VariableType>RegularExpression</VariableType>
            <Regex>xulrunner-([\d\.]+)\.</Regex>
            <Url>http://ftp.mozilla.org/pub/mozilla.org/xulrunner/releases/latest/runtimes/</Url>
            <Name>version</Name>
          </UrlVariable>
        </value>
      </item>
      <item>
        <key>
          <string>url</string>
        </key>
        <value>
          <UrlVariable>
            <RegexRightToLeft>false</RegexRightToLeft>
            <VariableType>Textual</VariableType>
            <Regex />
            <TextualContent>http://ftp.mozilla.org/pub/mozilla.org/xulrunner/releases/{version}/runtimes/{getUrl}</TextualContent>
            <Name>url</Name>
          </UrlVariable>
        </value>
      </item>
      <item>
        <key>
          <string>getUrl</string>
        </key>
        <value>
          <UrlVariable>
            <RegexRightToLeft>false</RegexRightToLeft>
            <VariableType>RegularExpression</VariableType>
            <Regex>[^ "'&lt;&gt;\*]+\.zip</Regex>
            <Url>http://ftp.mozilla.org/pub/mozilla.org/xulrunner/releases/{version}/runtimes/</Url>
            <Name>getUrl</Name>
          </UrlVariable>
        </value>
      </item>
      <item>
        <key>
          <string>checksum</string>
        </key>
        <value>
          <UrlVariable>
            <RegexRightToLeft>false</RegexRightToLeft>
            <VariableType>RegularExpression</VariableType>
            <Regex>([A-Fa-f0-9]{40})\s*runtimes/xulrunner-{version}[^\r\n]*?win32.zip</Regex>
            <Url>http://ftp.mozilla.org/pub/mozilla.org/xulrunner/releases/{version}/SHA1SUMS</Url>
            <TextualContent>{checksum}</TextualContent>
            <Name>checksum</Name>
          </UrlVariable>
        </value>
      </item>
